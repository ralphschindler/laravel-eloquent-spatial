{
    "name": "ralphschindler/laravel-eloquent-spatial",
    "description": "A Spatial Additions for Laravel Eloquent Models",
    "type": "library",
    "authors": [
        {
            "name": "Ralph Schindler",
            "email": "ralph@ralphschindler.com"
        }
    ],
    "require": {
        "laravel/framework": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEloquentSpatial\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelEloquentSpatial\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEloquentSpatial\\EloquentSpatialProvider"
            ]
        }
    }
}
